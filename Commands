"system information"	"uname -a"
"kernel version"	"uname -r"
"host name"	"hostname"
"host IP"	"hostname -I"
"reboot history"	"last reboot"
"date"	"date"
"calendar"	"cal"
"who online"	"w"
"remove file"	"rm file"
"copy between files"	"cp file1 file2"
"create symbolic link"	"ln -s /path/to/file linkname"
"view file contents"	"cat file"
"browse text file"	"less file"
"who logged"	"who"
"recently logged"	"last"
"info disk"	"hdparm -i /dev/sda"
"PCI devices"	"lspci -tv"
"Install package"	"yum install package"
"information package"	"yum info package"
"uninstall package"	"yum remove package"
"create directory"	"mkdir directory"
"remove directory"	"rm -r directory"
